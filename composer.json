{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "drobee/nova-sluggable": "^1.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "inspheric/nova-url-field": "^1.4",
        "laravel-frontend-presets/tall": "^1.7",
        "laravel/framework": "^7.0",
        "laravel/nova": "^3.6",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravel/vapor-core": "^2.7",
        "livewire/livewire": "^1.1",
        "mdixon18/fontawesome": "^0.1.1",
        "rmsj/nova-hero-icon": "^1.0",
        "spatie/laravel-cookie-consent": "^2.12"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/helo-laravel": "^1.0",
        "beyondcode/tinkerwell-helper": "^1.1",
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "sensiolabs/security-checker": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix",
            "@php artisan ide-helper:models -W",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:generate"
        ],
        "ide-helper": [
            "@php artisan ide-helper:models -W",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:generate"
        ],
        "check-security": [
            "vendor/bin/security-checker security:check"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
